{
  "name": "react_ssr",
  "version": "1.0.0",
  "description": "A node server to ssr our react files.",
  "main": "./dist/views/prerender.js",
  "scripts": {
    "build": "sass views/styles:public/css && tsc && vite build && node dist/views/prerender.js",
    "prerender": "node dist/views/prerender.js",
    "nodemon": "nodemon prerender.ts --exec 'tsc && vite build && node dist/prerender.js' -e ts,tsx --ignore node_modules/ --ignore dist/",
    "sass": "sass views/styles:public/css --watch",
    "start": "node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html-react-parser": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "redis": "^4.6.8"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.4",
    "nodemon": "^3.0.1",
    "sass": "^1.54.9",
    "typescript": "^5.1.3",
    "vite": "^4.4.9"
  }
}
